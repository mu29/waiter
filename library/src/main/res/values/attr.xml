<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="WaitingView">
        <attr name="waiting" format="boolean"/>
        <attr name="waitingColor" format="reference"/>
        <attr name="waitingWidth" format="dimension"/>
        <attr name="waitingHeight" format="dimension"/>
        <attr name="waitingMargin" format="dimension"/>
        <attr name="waitingMarginTop" format="dimension"/>
        <attr name="waitingMarginBottom" format="dimension"/>
        <attr name="waitingMarginStart" format="dimension"/>
        <attr name="waitingMarginEnd" format="dimension"/>
        <attr name="waitingPosition">
            <flag name="top" value="48"/>
            <flag name="bottom" value="80"/>
            <flag name="left" value="3"/>
            <flag name="right" value="5"/>
            <flag name="center_vertical" value="16"/>
            <flag name="center" value="17"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="WaitingButton">
    </declare-styleable>
</resources>
